{
  "author": {
    "name": "Brian Lange",
    "email": "bwlange@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bwlange/sym-pick.git"
  },
  "name": "sym-pick",
  "publisher": "the-repo-mule",
  "displayName": "Sym Pick",
  "description": "Sym-Pick is a lightweight and efficient VS Code extension that makes inserting Unicode symbols effortless. It provides a searchable list of symbols and emojis that can be inserted into your code with a single click.",
  "version": "1.0.1",
  "icon": "./images/icon.png",
  "bugs": {
		"url": "https://github.com/bwlange/sym-pick/issues",
		"email": "bwlange@gmail.com"
	},
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      { "id": "php" },
      { "id": "html" },
      { "id": "javascript" },
      { "id": "javascriptreact" },
      { "id": "css" },
      { "id": "git-commit" },
      { "id": "go" },
      { "id": "java" },
      { "id": "julia" },
      { "id": "markdown" },
      { "id": "jade" },
      { "id": "python" },
      { "id": "scss" },
      { "id": "typescript" },
      { "id": "typescriptreact" },
      { "id": "vb" },
      { "id": "xml" },
      { "id": "vue" },
      { "id": "asciidoc" },
      { "id": "apex" },
      { "id": "csharp" },
      { "id": "c" },
      { "id": "cpp" },
      { "id": "clojure" },
      { "id": "coffee" },
      { "id": "dart" },
      { "id": "dockerfile" },
      { "id": "elixir" },
      { "id": "elm" },
      { "id": "erlang" },
      { "id": "fsharp" },
      { "id": "graphql" },
      { "id": "groovy" },
      { "id": "haskell" },
      { "id": "handlebars" },
      { "id": "hcl" },
      { "id": "jsonc" },
      { "id": "kotlin" },
      { "id": "less" },
      { "id": "lua" },
      { "id": "makefile" },
      { "id": "nim" },
      { "id": "objective-c" },
      { "id": "perl" },
      { "id": "perl6" },
      { "id": "pgsql" },
      { "id": "powershell" },
      { "id": "r" },
      { "id": "ruby" },
      { "id": "rust" },
      { "id": "scala" },
      { "id": "shellscript" },
      { "id": "solidity" },
      { "id": "sql" },
      { "id": "stylus" },
      { "id": "swift" },
      { "id": "tcl" },
      { "id": "twig" },
      { "id": "yaml" },
      { "id": "forcesourcemanifest" }
    ],
    "commands": [
      {
        "command": "extension.showSymbolCompletion",
        "title": "Show Symbol Completion",
        "category": "Symbols"
      }
    ],
    "keybindings": [
      {
        "command": "extension.showSymbolCompletion",
        "key": "ctrl+alt+s",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "esbuild": "^0.24.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
